<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.GrandmasSecrets"
        parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#C1AC90</item>
        <item name="colorPrimaryDark">#9B7C57</item>
        <item name="android:textColorPrimary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimary</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="specific_rounded_corner">
        <item name="cornerSizeTopLeft">50dp</item>
        <item name="cornerSizeBottomRight">50dp</item>
        <item name="cornerFamilyTopLeft">rounded</item>
        <item name="cornerFamilyBottomRight">rounded</item>
    </style>
</resources>